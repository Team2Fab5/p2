<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">

        <h2>{{msg}}</h2>

        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">What's your name?</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Liz Lemon">
          </div>
          <div class="form-group">
            <label for="example-description">Where do you live?</label>
            <input class="form-control" id="example-description" aria-describedby="example-description" placeholder="123 Main St. Chicago, IL"></input>

          </div>
          <button id="submit" class="btn btn-light float-right">
            <img id="buttonImg" src="img/search.png" />
          </button>
        </form>
        <h3>Wow, look at this beautiful map underneath this text:</h3>
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>